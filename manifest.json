{
	"manifest_version": 3,
	"name": "Docs+",
	"description": "Improved Google docs",
	"version": "4.3",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"permissions": ["tabs", "identity", "storage"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content-script.js"]
		}
	]
}
